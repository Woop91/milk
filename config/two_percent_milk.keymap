#include <behaviors.dtsi>

/ {
    combos {
        compatible = "zmk,combos";

        de {
            bindings = <&mt A D>;
            key-positions = <0 1>;
        };
    };

    keymap {
        compatible = "zmk,keymap";

        default_layer {
            bindings = <
&mt K_POWER C_PP  &mt ESC ENTER
            >;
        };
    };
};
